def expression(m):
  z = (m**0.5) + 10
  return z


def middle_ar(x, y):
  total = 0
  count = 0

  for number in range(x, y + 1):
    if number % 2 == 0:
      total += number
      count += 1

  if count == 0:
    return "У заданому діапазоні немає парних чисел"
  else:
    average = total / count
    return average


